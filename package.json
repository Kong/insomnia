{
  "name": "insomnia",
  "private": true,
  "version": "1.0.0",
  "description": "Insomnia App",
  "main": "electron.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/gschier/insomnia"
  },
  "scripts": {
    "start-hot": "HOT=1 NODE_ENV=development electron -r babel-register ./app/app.js",
    "hot-server": "node -r babel-register ./webpack/server.js",
    "dev": "concurrently --kill-others \"npm run hot-server\" \"npm run start-hot\"",
    "build": "bash scripts/build.sh",
    "package": "npm run build && bash scripts/package.sh"
  },
  "build": {
    "appId": "com.insomnia.app",
    "app-category-type": "public.app-category.developer-tools",
    "compression": "normal",
    "win": {
      "iconUrl": "https://trello-logos.s3.amazonaws.com/06507f4661a7bb6375a8cf5beeaa48be/170.png",
      "icon": "build/icon.ico",
      "loadingGif": "build/install-spinner.gif",
      "remoteReleases": "https://s3.amazonaws.com/builds-insomnia-rest/win"
    }
  },
  "directories": {
    "app": "build",
    "output": "dist"
  },
  "dependencies": {
    "analytics-node": "^2.1.0",
    "classnames": "^2.2.3",
    "codemirror": "^5.12.0",
    "electron-squirrel-startup": "^1.0.0",
    "json-lint": "^0.1.0",
    "jsonschema": "^1.1.0",
    "mousetrap": "^1.6.0",
    "nedb": "^1.8.0",
    "nunjucks": "^1.3.4",
    "raven": "^0.11.0",
    "react": "^15.2.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.1",
    "react-tabs": "^0.5.5",
    "redux": "^3.3.1",
    "redux-logger": "^2.6.1",
    "redux-shortcuts": "0.0.1",
    "redux-thunk": "^2.0.1",
    "request": "^2.74.0"
  },
  "devDependencies": {
    "babel-core": "^6.7.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-object-rest-spread": "^6.6.5",
    "babel-polyfill": "^6.7.2",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-react-hmre": "^1.1.1",
    "concurrently": "^2.0.0",
    "css-loader": "^0.23.1",
    "electron-builder": "^5.12.1",
    "electron-packager": "^7.3.0",
    "electron-prebuilt": "^1.2.6",
    "express": "latest",
    "file-loader": "^0.8.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^1.3.0",
    "redux-mock-store": "^1.0.2",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-middleware": "latest",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.10.0"
  },
  "devEngines": {
    "node": "4.x || 5.x",
    "npm": "2.x || 3.x"
  }
}
