    "@types/react": "^16.14.5",
    "@types/react-dom": "^16.9.12",
    "@types/url-join": "^4.0.1",
    "axios": "^0.21.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "esbuild": "^0.14.29",
    "esbuild-runner": "^2.2.1",
    "rimraf": "3.0.2",
    "typescript": "^4.5.5"
  }
