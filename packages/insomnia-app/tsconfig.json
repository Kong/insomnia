{
  "compilerOptions": {
    "types": ["vite/client"],
    "outDir": ".",
    "rootDir": ".",

    "strict": false,

    "target": "esnext",
    "module": "esnext",
    "isolatedModules": true,
    "moduleResolution": "node",
    "esModuleInterop": false,
    "resolveJsonModule": true,
    "experimentalDecorators": true,

    "allowSyntheticDefaultImports": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "jsx": "react-jsx"
  },
  "include": [
    "package.json"
  ],
  "exclude": []
}
