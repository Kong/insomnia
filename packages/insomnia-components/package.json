    "class-autobind-decorator": "^3.0.1",
    "classnames": "^2.3.1",
    "framer-motion": "^1.10.3",
    "fuzzysort": "^1.1.4",
    "md5": "^2.2.1",
    "react-switch": "^5.0.1"
  },
