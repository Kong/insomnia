    "insomnia-plugin-jsonpath": "2.3.1",
    "insomnia-plugin-now": "2.3.1",
    "insomnia-plugin-os": "2.3.1",
    "insomnia-plugin-prompt": "2.3.1",
    "insomnia-plugin-request": "2.3.1",
    "insomnia-plugin-response": "2.3.1",
    "insomnia-plugin-uuid": "2.3.1",
    "insomnia-send-request": "2.3.1",
    "insomnia-testing": "2.3.1",
    "lodash.flattendeep": "^4.4.0",
    "mkdirp": "^1.0.4",
    "nedb": "^1.8.0",
    "openapi-2-kong": "2.3.1",
    "string-argv": "^0.3.1",
    "yaml": "^1.10.0"
  }
