{
  "services": [
    {
      "protocol": "https",
      "host": "USPTO_Data_Set_API",
      "port": 443,
      "path": "/ds-api",
      "name": "USPTO_Data_Set_API",
      "plugins": [],
      "routes": [
        {
          "methods": ["GET"],
          "strip_path": false,
          "tags": ["OAS3_import", "OAS3file_uspto.yaml"],
          "name": "list-data-sets",
          "paths": ["/$"]
        },
        {
          "methods": ["GET"],
          "strip_path": false,
          "tags": ["OAS3_import", "OAS3file_uspto.yaml"],
          "name": "list-searchable-fields",
          "paths": ["\/(?<dataset>[^\\\/\\s]+)\/(?<version>[^\\\/\\s]+)\/fields$"]
        },
        {
          "methods": ["POST"],
          "strip_path": false,
          "tags": ["OAS3_import", "OAS3file_uspto.yaml"],
          "name": "perform-search",
          "paths": ["\/(?<dataset>[^\\\/\\s]+)\/(?<version>[^\\\/\\s]+)\/records$"]
        }
      ],
      "tags": ["OAS3_import", "OAS3file_uspto.yaml"]
    }
  ],
  "upstreams": [
    {
      "tags": ["OAS3_import", "OAS3file_uspto.yaml"],
      "name": "USPTO_Data_Set_API",
      "targets": [
        {
          "target": "developer.uspto.gov:443",
          "tags": ["OAS3_import", "OAS3file_uspto.yaml"]
        }
      ]
    }
  ],
  "_format_version": "1.1"
}
