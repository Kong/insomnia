{
  "services": [
    {
      "url": "http://petstore.swagger.io/v1",
      "name": "Swagger_Petstore",
      "plugins": [],
      "routes": [
        {
          "methods": ["GET"],
          "strip_path": false,
          "tags": ["OAS3_import", "OAS3file_petstore.yaml"],
          "name": "listPets",
          "paths": ["/pets$"]
        },
        {
          "methods": ["POST"],
          "strip_path": false,
          "tags": ["OAS3_import", "OAS3file_petstore.yaml"],
          "name": "createPets",
          "paths": ["/pets$"]
        },
        {
          "methods": ["GET"],
          "strip_path": false,
          "tags": ["OAS3_import", "OAS3file_petstore.yaml"],
          "name": "showPetById",
          "paths": ["\/pets\/(?<petId>[^\\\/\\s]+)$"]
        }
      ],
      "tags": ["OAS3_import", "OAS3file_petstore.yaml"]
    }
  ],
  "upstreams": [
    {
      "tags": ["OAS3_import", "OAS3file_petstore.yaml"],
      "name": "Swagger_Petstore",
      "targets": [{"target": "petstore.swagger.io:80"}]
    }
  ],
  "_format_version": "1.1"
}
