{
	"_format_version": "1.1",
	"services": [
		{
			"name": "Testing_Circular",
			"protocol": "https",
			"host": "some.random.url",
			"port": 443,
			"path": "/",
			"plugins": [
				{
					"name": "request-validator",
					"config": {
						"version": "draft4",
						"body_schema": "{}",
						"verbose_response": true
					},
					"tags": [
						"OAS3_import",
            "OAS3file_test-circular.yaml"
					],
					"enabled": true
				}
			],
			"routes": [
				{
					"tags": [
						"OAS3_import",
            "OAS3file_test-circular.yaml"
					],
					"name": "Testing_Circular-testing-testingId-testing-endpoint-post",
					"methods": [
						"POST"
					],
					"paths": [
						"/testing/(?<testingId>[^\\/]+)/testing-endpoint$"
					],
					"strip_path": false,
					"plugins": [
						{
							"name": "request-validator",
							"config": {
								"version": "draft4",
								"parameter_schema": [
									{
										"in": "path",
										"explode": false,
										"required": true,
										"name": "testingId",
										"schema": "{\"type\":\"string\",\"maxLength\":50}",
										"style": "simple"
									}
								],
								"body_schema": "{\"description\":\"desc\",\"allOf\":[{\"description\":\"desc\",\"type\":\"object\",\"properties\":{\"entity1Prop1\":{\"description\":\"desc\",\"oneOf\":[{\"description\":\"desc\",\"allOf\":[{\"description\":\"desc\",\"type\":\"string\"},{\"description\":\"desc\",\"type\":\"string\",\"properties\":{\"owners\":{\"type\":\"array\",\"description\":\"desc\",\"items\":{\"description\":\"desc\",\"type\":\"object\",\"required\":[\"entity5\"],\"properties\":{\"entity5\":{\"description\":\"desc\",\"oneOf\":[{\"description\":\"desc\",\"allOf\":[{\"description\":\"desc\",\"allOf\":[null,null]}]}],\"discriminator\":{\"propertyName\":\"entity5Type\",\"mapping\":{\"ENTITY_3\":\"#/components/schemas/Entity3AsOwner\"}}}}}}}}]}],\"discriminator\":{\"propertyName\":\"entity5Type\",\"mapping\":{\"ENTITY_CIRCULAR_START\":\"#/components/schemas/EntityCircularStart\"}}}}}]}",
								"allowed_content_types": [
									"application/json"
								],
								"verbose_response": true
							},
							"tags": [
								"OAS3_import",
                "OAS3file_test-circular.yaml"
							],
							"enabled": true
						}
					]
				}
			],
			"tags": [
				"OAS3_import",
        "OAS3file_test-circular.yaml"
			]
		}
	]
}
