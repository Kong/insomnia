{
  "services": [
    {
      "protocol": "https",
      "host": "USPTO_Data_Set_API.upstream",
      "port": 443,
      "path": "/ds-api",
      "name": "USPTO_Data_Set_API",
      "plugins": [],
      "routes": [
        {
          "methods": ["GET"],
          "strip_path": false,
          "tags": ["OAS3_import", "OAS3file_uspto.yaml"],
          "name": "USPTO_Data_Set_API-list_data_sets",
          "paths": ["/$"]
        },
        {
          "methods": ["GET"],
          "strip_path": false,
          "tags": ["OAS3_import", "OAS3file_uspto.yaml"],
          "name": "USPTO_Data_Set_API-list_searchable_fields",
          "paths": ["/(?<dataset>[^\\/]+)/(?<version>[^\\/]+)/fields$"]
        },
        {
          "methods": ["POST"],
          "strip_path": false,
          "tags": ["OAS3_import", "OAS3file_uspto.yaml"],
          "name": "USPTO_Data_Set_API-perform_search",
          "paths": ["/(?<dataset>[^\\/]+)/(?<version>[^\\/]+)/records$"]
        }
      ],
      "tags": ["OAS3_import", "OAS3file_uspto.yaml"]
    }
  ],
  "upstreams": [
    {
      "tags": ["OAS3_import", "OAS3file_uspto.yaml"],
      "name": "USPTO_Data_Set_API.upstream",
      "targets": [
        {
          "target": "developer.uspto.gov:443",
          "tags": ["OAS3_import", "OAS3file_uspto.yaml"]
        }
      ]
    }
  ],
  "_format_version": "1.1"
}
