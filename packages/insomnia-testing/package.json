    "verbose": false,
    "resetMocks": true,
    "resetModules": true
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-optional-chaining": "^7.9.0",
    "husky": "^3.1.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11"
  }
}
