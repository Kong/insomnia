{
  "private": true,
  "name": "insomnia-changelog",
  "version": "2.4.1",
  "homepage": "https://insomnia.rest",
  "description": "General Purpose changelog generator, not just for insomnia",
  "author": "Kong <office@konghq.com>",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/Kong/insomnia.git",
    "directory": "packages/insomnia-changelog"
  },
  "bin": {
    "insomnia-changelog": "./dist/index.js"
  },
  "bugs": {
    "url": "https://github.com/kong/insomnia/issues"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "esr src/cli.ts generate --owner=Kong --repo=insomnia",
    "bootstrap": "npm run build",
    "lint": "eslint . --ext .js,.ts,.tsx --cache",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest",
    "clean": "tsc --build tsconfig.build.json --clean",
    "prebuild": "npm run clean",
    "build": "tsc --build tsconfig.build.json"
  },
  "dependencies": {
    "ramda": "^0.27.1",
    "ramda-adjunct": "2.33.0"
  },
  "devDependencies": {
    "@octokit/rest": "^18.12.0",
    "@octokit/types": "^6.34.0",
    "@types/ramda": "^0.27.44",
    "commander": "^9.0.0",
    "esbuild": "^0.14.2",
    "jest": "^26.6.3",
    "esbuild-runner": "^2.2.1",
    "type-fest": "^1.0.2",
    "typescript": "^4.2.3"
  }
}
